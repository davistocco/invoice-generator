{
    "extends": "@apify/tsconfig",
    "compilerOptions": {
        "module": "ESNext",
        "target": "ES2022",
        "outDir": "dist",
        "noUnusedLocals": false,
        "lib": [
            "DOM"
        ],
        "moduleResolution": "bundler",
        "allowImportingTsExtensions": true,
        "noEmit": true
    },
    "include": [
        "./src/**/*"
    ]
}